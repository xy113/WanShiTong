/* pages/shop/index.wxss */

.catlog-list{
  display: flex;
  width: 100%;
  flex-flow: row wrap;
  padding: 10px 0;
  background-color: #fff;
  margin-bottom: 10px;
}
.catlog-list .item{
  flex: 1;
  width: 20%;
  min-width: 20%;
  max-width: 20%;
  display: block;
  padding: 5px 0;
}
.catlog-list .icon{
  width: 50px;
  height: 50px;
  border-radius: 50%;
  display: block;
  margin: 0 auto;
}

.catlog-list .name{
  text-align: center;
  font-size: 13px;
  display: block;
  padding: 5px;
  font-weight: 400;
}

.pub-box .content{
  background-color: #00CA52;
}
